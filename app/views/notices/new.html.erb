<h1>notice#new</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@notice) do |f| %>
    
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>

      <%= f.label :content %>
      <%= f.text_field :content, class: 'form-control' %>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>